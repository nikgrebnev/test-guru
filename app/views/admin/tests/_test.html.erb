<tr>
  <td><%= test.id %></td>
  <td><%= test.category.title %></td>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('nav.show'), admin_test_path(test) %></td>
  <td><%= link_to t('nav.edit'), edit_admin_test_path(test) %></td>
